TARGET=3b2

SRCFILES = tartsrarpd.c

FILES = $(SRCFILES)

PROCESS = tartsrarpd

OBJECTS = $(SRCFILES:B:S=.o)

.MAIN:	$(PROCESS)

#include "../../../Cntlrules.mk"

.SOURCE.c:	..
.SOURCE.h:	.INSERT $(WINCCHDRS)
.SOURCE.a:	$(WINLIBDIR)

LD=3b2ld
STRIP=3b2strip
LDLIBRARIES+= $(WINLIBDIR)/libnet.a $(WINLIBDIR)/libnetdb.a $(WINLIBDIR)/libnsl_s.a

$(PROCESS):	$(OBJECTS)
	$(CC) $(LDFLAGS) $(*) $(LDLIBRARIES) -o $(<)


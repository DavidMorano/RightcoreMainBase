# DISTFILE (RDIST)


# host lists

HOSTS=(
	mtgzfs7
	mtgbs1
)


POSTLISTS=(
	/usr/postlists
)

BOARDS=(
	boards
)


VMAIL=(
	vmail30h
)


PCSMAIL=(
	pcsmail1e
)


# package lists

postlists:
	${POSTLISTS} -> hodia.info.att.com
		install -i -R /usr/add-on/pcs/lib/pcsmail/user.lists ;

boards:
	${BOARDS} -> hodih.info.att.com
		install -i -w /var/spool/pcs ;

vmail:
	${VMAIL} -> hocpx
		install -i -w /home/dam/src ;

pcsmail:
	${PCSMAIL} -> hocpx
		install -i -w /home/dam/src ;





